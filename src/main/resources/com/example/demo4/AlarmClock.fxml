<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<ScrollPane xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.AlarmClockController">
   <content>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextField fx:id="alarmTime" layoutX="226.0" layoutY="243.0" />
            <Text fx:id="timer" layoutX="289.0" layoutY="173.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
            <Label layoutX="269.0" layoutY="81.0" text="Alarm Clock" />
            <Label layoutX="14.0" layoutY="14.0" text="Mk III Alarm" />
            <Button fx:id="GoBack" layoutX="87.0" layoutY="10.0" mnemonicParsing="false" onMouseClicked="#GoMenu" text="Вернуться" />
            <Text fx:id="TextMessage" layoutX="123.0" layoutY="310.0" strokeType="OUTSIDE" strokeWidth="0.0" text="?" />
            <Label layoutX="73.0" layoutY="297.0" text="Message:" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
